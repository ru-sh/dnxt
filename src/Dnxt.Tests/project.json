{
	"version" : "0.1.1-rc2",
	"description" : "DotNet Extensions",
	"packOptions" : {
		"tags" : ["DotNet Extensions"],
		"projectUrl" : "https://github.com/ru-sh/dnxt"
	},

  "buildOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "Dnxt": "0.1.1-rc2",
    "Dnxt.DtoGeneration": "0.1.1-rc2",
    "Dnxt.Logging": "0.1.1-rc2",
    "Dnxt.Messaging.RabbitMq": "0.1.1-rc2",
    "RabbitMQ.Client.CoreClrUnofficial": "3.6.1.2-rc2",
    "NUnitLite": "3.2.1"  
  },

	"frameworks" : {

		"netcoreapp1.0" : {
			"imports" : [
				"dotnet5.6",
				"dnxcore50",
				"portable-net45+win8"
			],
			"buildOptions" : {
				"define" : ["CORECLR"]
			},
			"dependencies" : {
				"Microsoft.NETCore.App" : {
					"version" : "1.0.0-rc2-3002702",
					"type" : "platform"
				},
				"Newtonsoft.Json" : "8.0.2"
			}
		}
	}
}